{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": false,
        "member-ordering": false,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "no-string-literal": false,
        "no-var-requires": {
            "severity": "warning"
        },
        "quotemark": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "variable-name": false
    },
    "rulesDirectory": []
}
